{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
			"@/*": ["./src/*"],
			"@/components/*": ["./src/components/*"],
			"@/layouts/*": ["./src/layouts/*"],
			"@/lib/*": ["./src/lib/*"],
			"@/core/*": ["./src/modules/core/*"],
			"@/AUI/*": ["./src/modules/application-ui/*"],
			"@/ecommerce/*": ["./src/modules/ecommerce/*"],
			"@/marketing/*": ["./src/modules/marketing/*"]
		},
    "baseUrl": "./"
  }
}
